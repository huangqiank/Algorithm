def fib(i):
    if i == 0:
        return 0
    if i == 1:
        return 1
    else:
        return fib(i-1) + fib(i-2)
    
def fib2(i,sum=[0,1]):   
    if i == 0:
        return 0
    if i == 1:
        return 1      
    for j in xrange(2,i): 
        return fib2(j-1) + fib2(j-2)
