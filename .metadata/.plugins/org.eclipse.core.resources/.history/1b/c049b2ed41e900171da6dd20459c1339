a=["apple","b","c","d","sx","sd","das","sd","sx","bc","a","apple"]

def count_map(a):
    lst=[]
    for i in a:
        dict={}
        dict[i]=1
        lst.append((dict)) 
    return lst

def reduce(a):
    dict = {}
    for i in a:
        if i.keys not in dict:
            dict[i] = 1
        else:
            dict[i] += 1
    return dict
print reduce(count_map(a))
        
        
        
        


        
        
            