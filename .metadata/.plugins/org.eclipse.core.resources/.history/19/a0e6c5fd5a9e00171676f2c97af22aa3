def fib(i):
    if i ==0:
        return 0 
    if  i == 1:
        return 1
    return fib(i-1)+fib(i-2)
def fib2(i,s=[0,1]):
    if i ==0:
        return 0 
    if  i == 1:
        return 1
    for j in xrange(2,i+1):
        s[j] = s[j-1] + s[j-2]
    print s[i]
    
fib2(3)
